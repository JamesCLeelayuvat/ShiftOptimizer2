<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.XMLControllers.StartScreenController" spacing="10" alignment="CENTER">
  <padding>
    <Insets top="20" right="20" bottom="20" left="20"/>
  </padding>

  <Button text="Sign Up" fx:id="signUpButton" onAction="#handleSignUp"/>
  <Button text="Sign In" fx:id="signInButton" onAction="#handleSignIn"/>
</VBox>
